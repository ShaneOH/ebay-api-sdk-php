<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:xsd="http://testing-namespace"
  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">

  <wsdl:types>
    <xs:schema>

      <xs:complexType name="ComplexType">
        <xs:complexContent>
          <xs:extension base="tns:AnotherType">
            <xs:sequence>
              <xs:element name="foo"/>
              <xs:element name="bar"/>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>

      <xsd:complexType name="AnotherType">
        <xs:sequence>
          <xs:element name="foo"/>
          <xs:element name="bar"/>
        </xs:sequence>
      </xsd:complexType>

      <!-- 
        These elements contain complexType with no names.
        If we transform these by mistake we will end up trying to create two files with 
        the same name which results in the error below.

        Cannot write more than one result document to the same URI
      -->
      <xs:element>
        <xs:complexType>
        </xs:complexType>
      </xs:element>

      <xs:element>
        <xs:complexType>
        </xs:complexType>
      </xs:element>

    </xs:schema>
  </wsdl:types>
</wsdl:definitions>
