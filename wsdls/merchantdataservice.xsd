            </CallInfo>
            <CallInfo>
              <CallName>GetSellerEvents</CallName>
              <Details>DetailLevel: ReturnAll</Details>
              <Returned>Conditionally</Returned>
            </CallInfo>
          </xs:appinfo>
        </xs:annotation>
