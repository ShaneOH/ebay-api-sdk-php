      <xs:simpleType name="PaypalSNADCaseStatusType">
        <xs:annotation>
          <xs:documentation>
			Enumeration type that contains all status values applicable to a Significantly Not As Described case opened by a
			buyer in the PayPal system.		
		</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
          <xs:enumeration value="OPEN">
            <xs:annotation>
              <xs:documentation>
					The case is open.
				</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="CLOSED">
            <xs:annotation>
              <xs:documentation>
					The case is closed.
				</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="OTHER_PARTY_RESPONSE_DUE">
            <xs:annotation>
              <xs:documentation>
